package com.hucloud.di.sample8.vo;

import java.util.List;

public class PersonVO {

	private String name;
	private String age;
	private List<String> usableLanguage;
	
	public PersonVO(String name, String age, List<String> usableLanguage) {
		this.name = name;
		this.age = age;
		this.usableLanguage = usableLanguage;
	}
	
	public String getName() {
		return name;
	}
	
	public String getAge() {
		return age;
	}
	
	public List<String> getUsableLanguage() {
		return usableLanguage;
	}
	
}
